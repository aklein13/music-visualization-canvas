{
  "name": "music-visualization",
  "version": "1.0.0",
  "description": "music visualization",
  "main": "lib/index.js",
  "scripts": {
    "dev": "parcel ./src/index.html --open",
    "build:lib": "parcel ./lib/index.ts --out-dir lib --no-source-maps --no-hmr --no-cache --global MusicVisualization",
    "build": "parcel build ./src/index.html --public-url ./ --out-dir dist --no-source-maps",
    "deploy": "rm -r dist && npm run build && gh-pages -d dist"
  },
  "keywords": [
    "music",
    "canvas"
  ],
  "author": "mengxiong10",
  "license": "ISC",
  "dependencies": {
    "lodash": "^4.17.11"
  },
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/preset-env": "^7.2.3",
    "@typescript-eslint/eslint-plugin": "^1.5.0",
    "@typescript-eslint/parser": "^1.5.0",
    "cssnano": "^4.1.8",
    "eslint": "^5.3.0",
    "eslint-config-prettier": "^4.1.0",
    "gh-pages": "^2.0.1",
    "parcel-bundler": "^1.12.3",
    "typescript": "^3.5.3"
  }
}
